{
  "name": "ng-jest-spec",
  "version": "1.0.1",
  "description": "Package for generating bare-bone structure for unit test spec files!",
  "main": "dist/bundle.js",
  "module": "dist/bundle.js",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development --watch",
    "test": "jest --silent",
    "test-coverage": "jest --silent --coverage"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^5.55.0",
    "async": "3.2.4",
    "chalk": "^4.1.2",
    "prompts": "^2.4.2",
    "typescript": "5.0.3"
  },
  "keywords": [
    "node",
    "angular",
    "ng",
    "typescript",
    "jest",
    "plugin",
    "template",
    "spec",
    "builder",
    "generator"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "./test/example/*"
    ]
  },
  "author": "Abhishek P N",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@types/jest": "^29.5.2",
    "@webpack-cli/generators": "^2.5.0",
    "babel-loader": "^8.2.5",
    "coveralls": "^3.1.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "type": "commonjs",
  "bin": {
    "ng-jest-spec": "./dist/bundle.js"
  }
}
