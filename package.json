{
  "name": "ng-test-gen",
  "version": "1.0.0",
  "description": "Package for generating bare-bone structure for jest unit test spec files!",
  "main": "./src/prompt.js",
  "scripts": {
    "test": "jest --silent",
    "test-coverage": "jest --silent --ci --collectCoverage=true --coverageReporters=lcov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asaralaya/jest-template"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^5.55.0",
    "async": "3.2.4",
    "chalk": "^4.1.2",
    "prompts": "^2.4.2",
    "typescript": "5.0.3"
  },
  "keywords": [
    "node",
    "typescript",
    "jest",
    "plugin",
    "template",
    "generator"
  ],
  "jest": {
    "testPathIgnorePatterns": ["./test/example/*"]
  },
  "author": "Abhishek P N",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "coveralls": "^3.1.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4"
  },
  "type": "commonjs",
  "bin": {
    "jest-template": "./src/prompt.js"
  }
}
